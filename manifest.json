{
  "manifest_version": 2,

  "name": "Comment Tagger",
  "description": "Logs the most commonly used words in Youtube comments",
  "version": "1.0",
  "icons": { "16": "icon.png",
           "48": "icon.png",
          "128": "icon.png" },  
  
  "content_scripts": [
	{
	  "matches": ["http://youtube.com/*"],
      "js": ["jQuery.min.js","tagger.js"]
	}
  ]
  
}